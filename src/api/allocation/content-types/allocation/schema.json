{
  "kind": "collectionType",
  "collectionName": "allocations",
  "info": {
    "singularName": "allocation",
    "pluralName": "allocations",
    "displayName": "allocation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true,
      "maxLength": 10000,
      "minLength": 1
    },
    "creator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "isPublic": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false
    },
    "company": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 1000,
      "minLength": 1
    },
    "companyImageUrl": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "maxLength": 10000
    },
    "recruitmentStages": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "recruitment.stages"
    },
    "applications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::application.application",
      "mappedBy": "allocation"
    },
    "isRemote": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean"
    },
    "applicationURL": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "maxLength": 999
    },
    "openPositions": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
    },
    "automaticClosingDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime"
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "active",
        "closed",
        "cancelled",
        "frozen"
      ],
      "default": "active"
    },
    "applicationEmail": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "maxLength": 999
    }
  }
}
